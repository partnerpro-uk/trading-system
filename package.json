{
  "name": "next-temp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "kill": "lsof -ti:3000 | xargs kill -9 2>/dev/null || echo 'No process on port 3000'",
    "restart": "npm run kill && npm run dev",
    "setup:timescale": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx tsx scripts/setup-timescale.ts",
    "setup:clickhouse": "npx tsx scripts/setup-clickhouse.ts",
    "migrate": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx tsx scripts/migrate/run-migration.ts",
    "migrate:candles-ch": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx tsx scripts/migrate/export-candles-to-clickhouse.ts",
    "migrate:candles-ts": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx tsx scripts/migrate/export-recent-to-timescale.ts",
    "migrate:news": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx tsx scripts/migrate/export-news-to-databases.ts",
    "verify-migration": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx tsx scripts/migrate/verify-migration.ts",
    "backfill:oanda": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx tsx scripts/migrate/backfill-candles-from-oanda.ts",
    "migrate:sessions": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx tsx scripts/migrate/export-sessions-to-timescale.ts",
    "worker:start": "cd worker && npm start",
    "worker:test": "cd worker && NODE_TLS_REJECT_UNAUTHORIZED=0 npx tsx src/test-connection.ts"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.59.6",
    "@clerk/nextjs": "^6.36.10",
    "@clickhouse/client": "^1.16.0",
    "@clickhouse/client-web": "^1.16.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "lightweight-charts": "^5.1.0",
    "lightweight-charts-line-tools": "^4.1.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.3",
    "pg": "^8.17.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-resizable-panels": "^4.4.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
